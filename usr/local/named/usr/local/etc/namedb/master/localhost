$TTL	10800
$ORIGIN	localhost.

@	IN	SOA	localhost.	dnsmaster.localhost.	(
			200		; Serial
			1d		; Refresh
			3h		; Retry
			18w		; Expire
			3h		; Negative cache TTL
			)

@	IN	NS	localhost.
@	IN	A	127.0.0.1

; #########################################################################
